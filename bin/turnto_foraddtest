#!/usr/bin/zsh
cd tmp


j=1;
case $# in 
2)
	for i in $(cat ../list.csv | fzf -1 -q "$*" --bind load:accept);
	do
		a[j]=$i;	 
		((j=$j+1))
	done
	;;
*)
	echo "turnto_foraddtest error!" | tee ../log/added.log
	exit 1;
	;;
esac

if [ "$a[1]" != "" ]; then
	echo $a[1] > SF; echo $a[2] > SFe; echo $a[3] > TR; echo $a[4] > TRe; echo $a[5] > G; echo $a[7] > S; 
echo $a[6] > Ge; echo $a[8] > Se; echo $a[9] > NO; echo $a[10] > Pm; echo $a[11] > Pf;
fi
echo "Keep `cat SF; cat TR; cat G; cat S`" | tee ../log/added.log
